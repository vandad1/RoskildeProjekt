<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<Scene xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MyCalendercontroller">
<AnchorPane prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label layoutX="14.0" layoutY="14.0" prefHeight="26.0" prefWidth="88.0" text="Logged in as: ">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Button layoutX="528.0" layoutY="8.0" mnemonicParsing="false" onMouseClicked="#logout" style="-fx-background-color: 0;" text="L o g  o u t" textFill="BLACK">
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
            <font>
                <Font size="14.0" />
            </font></Button>
        <Button layoutX="14.0" layoutY="40.0" mnemonicParsing="false" onMouseClicked="#GoBackMenuV" style="-fx-background-color: 0;" text="←">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Button>
         <ChoiceBox fx:id="Timepicker" layoutX="212.0" layoutY="175.0" prefHeight="28.0" prefWidth="63.0" />
         <DatePicker fx:id="DatePicker" layoutX="15.0" layoutY="176.0" />
         <Label alignment="CENTER" layoutY="70.0" prefHeight="18.0" prefWidth="604.0" text="Calendar" textAlignment="CENTER">
            <font>
               <Font size="24.0" />
            </font>
         </Label>
         <Label layoutX="15.0" layoutY="158.0" prefHeight="18.0" prefWidth="40.0" text="Date" />
         <Label layoutX="212.0" layoutY="159.0" text="Time" />




    </children>
</AnchorPane>
</Scene>
