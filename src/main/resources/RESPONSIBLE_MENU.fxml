<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Scene xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RESPONSIBLEMENUcontroller">
<AnchorPane prefHeight="400.0" prefWidth="600.0">
      <style>
         -fx-background-image: url("https://roskildefestivalgruppen.dk/media/1165/frivillig-small.jpg?width=1170");
         -fx-background-size: cover, auto;
            </style>
      <children>
         <Label layoutX="14.0" layoutY="14.0" prefHeight="26.0" prefWidth="88.0" text="Logged in as: " textFill="WHITE">
            <font>
               <Font size="14.0" />
            </font>
            <cursor>
               <Cursor fx:constant="DEFAULT" />
            </cursor>
         </Label>
         <Button layoutX="528.0" layoutY="27.0" mnemonicParsing="false" onMouseClicked="#logout" style="-fx-background-color: 0;" text="L o g  o u t" textFill="WHITE">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
         <Button layoutX="238.0" layoutY="132.0" mnemonicParsing="false" onMouseClicked="#CREATEVOLUNTEER" prefHeight="23.0" prefWidth="123.0" style="-fx-background-color: 0;" text="Create Volunteer" textFill="WHITE">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
         <Button layoutX="236.0" layoutY="172.0" mnemonicParsing="false" onMouseClicked="#EDITVOLUNTEER" prefHeight="27.0" prefWidth="128.0" style="-fx-background-color: 0;" text="Edit Volunteer" textFill="WHITE">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
         <Button layoutX="237.0" layoutY="214.0" mnemonicParsing="false" onMouseClicked="#CreateShift" prefHeight="23.0" prefWidth="126.0" style="-fx-background-color: 0;" text="Create Shift" textFill="WHITE">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
         <Button layoutX="238.0" layoutY="258.0" mnemonicParsing="false" onMouseClicked="#EditShift" prefHeight="22.0" prefWidth="124.0" style="-fx-background-color: 0;" text="Edit Shift" textFill="WHITE">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
      </children>
</AnchorPane>
</Scene>
